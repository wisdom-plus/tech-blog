{
  "extends": [
    "next/core-web-vitals",
    "plugin:import/typescript",
    "plugin:import/recommended",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "plugins": ["import", "unused-imports"],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "@next/next/no-html-link-for-pages": ["error", "src/app"]
  }
}
